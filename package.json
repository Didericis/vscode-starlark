{
  "name": "starlark",
  "displayName": "Starlark",
  "description": "Syntax highlighting for Starlark",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Didericis/vscode-starlark.git"
  },
  "author": {
    "name": "Didericis"
  },
  "license": "Apache-2.0",
  "icon": "media/bazel-logo.png",
  "contributes": {
    "languages": [{
      "id": "starlark",
      "aliases": ["Starlark", "starlark"],
      "extensions": [".star"],
      "configuration": "./language-configuration.json",
      "icon": {
        "dark": "./media/bazel-logo.png",
        "light": "./media/bazel-logo.png"
      }
    }],
    "grammars": [{
      "language": "starlark",
      "scopeName": "source.starlark",
      "path": "./syntaxes/starlark.tmLanguage.json"
    }]
  }
}
