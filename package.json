{
  "name": "starlark",
  "displayName": "Starlark",
  "description": "Syntax highlighting for Starlark",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "media/bazel-logo.png",
  "contributes": {
    "languages": [{
      "id": "starlark",
      "aliases": ["Starlark", "starlark"],
      "extensions": [".star"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "starlark",
      "scopeName": "source.starlark",
      "path": "./syntaxes/starlark.tmLanguage.json"
    }]
  }
}
